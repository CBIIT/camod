<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.camod.domain">
	<class name="Treatment" 
    	table="treatment"
        lazy="true">

	    <!-- id -->
	    <id name="id" 
	    	column="treatment_id"
	    	type="long"    	
	    	unsaved-value="null">	    	
	        <generator class="native"/>
	    </id>	    
	    
	    <!-- regimen -->
        <property name="regimen" 
        	column="regimen" 
        	type="string"/>
        	
        <!-- dosage -->
        <property name="dosage" 
        	column="dosage" 
        	type="string"/>
        	
        <!-- administrativeRoute -->
        <property name="administrativeRoute" 
        	column="administrative_route" 
        	type="string"/>	
        
        <!-- adminRouteUnctrlVocab -->
        <property name="adminRouteUnctrlVocab" 
        	column="admin_route_unctrl_vocab" 
        	type="string"/>	        
        
        <!-- ageAtTreatment -->
        <property name="ageAtTreatment" 
        	column="age_at_treatment" 
        	type="string"/>	
        	
        <!-- sexDistribution (many-to-one unidirectional) -->
        <many-to-one 
        	name="sexDistribution"
        	column="sex_distribution_id"
        	class="SexDistribution"/>
	</class>
</hibernate-mapping>