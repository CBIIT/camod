<?xml version="1.0" encoding="UTF-8"?>

<!--
    Document   : ExpressionLevelDesc.hbm.xml
    Created on : July 19, 2005
    Author     : rajputs
    Description: Hibernate mapping file for ExpressionLevelDesc
-->

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.camod.domain">
	<class name="ExpressionLevelDesc" 
    	table="exp_level_desc"
        lazy="true">

	    <!-- id -->
	    <id name="id" 
	    	column="exp_level_desc_id"
	    	type="long"    	
	    	unsaved-value="null">	    	
	        <generator class="native"/>
	    </id>
	    
	    <!-- expressionLevel -->
        <property name="expressionLevel" 
        	column="expression_level" 
        	type="string"/>
        	
        <!-- organCollection (many-to-many bidirectional) -->
        <!-- Note: this collection will NOT be used to synchronize state with the database. -->
        <!-- Changes made to expressionLevelDescCollection in Organ will be synchronized with the database. -->
        <bag name="organCollection"
        	table="organ_exp_level_desc"
        	inverse="true" 
        	lazy="true"
        	cascade="all-delete-orphan">
        	
        	<key column="exp_level_desc_id"/>
        	<many-to-many class="Organ" 
        		column="organ_id"/>
        </bag> 
	</class>
</hibernate-mapping>