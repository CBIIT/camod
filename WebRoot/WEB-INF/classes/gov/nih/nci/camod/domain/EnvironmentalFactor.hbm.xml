    Description: Hibernate mapping file for EnvironmentalFactor
-->

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.camod.domain">
	<class name="EnvironmentalFactor"
		table="env_factor"
        lazy="true">
	    
	    <!-- id -->
	    <id name="id" 
	    	column="env_factor_id"
	    	type="long"    	
	    	unsaved-value="null">	    	
	        <generator class="native"/>
	    </id>	
	    
	    <!-- discriminator -->
	    <discriminator column="env_factor_type"
	    	type="string"/>
	    
        <!-- type -->
        <property name="type" 
        	column="type" 
        	type="string"/>
        
        <!-- name -->
        <property name="name" 
        	column="name" 
        	type="string"/>
        	
        <!-- CASNumber -->
        <property name="casNumber" 
        	column="cas_number" 
        	type="string"/>
        	
        <!-- inducedMutationCollection (one-to-many bidirectional) -->
        <bag name="inducedMutationCollection" 
        	table="env_fac_ind_mutation"
        	inverse="false" 
        	lazy="true" 
        	cascade="all-delete-orphan">
        	
        	<key column="env_factor_id"/>
        	<many-to-many column="engineered_gene_id"
            	unique="true"
            	class="InducedMutation"/>
        </bag>
	</class>
</hibernate-mapping>