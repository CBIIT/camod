<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>
     <global>
      <constant>
        <constant-name>phone</constant-name>
        <constant-value>^\(?(\d{3})\)?[-| ]?(\d{3})[-| ]?(\d{4})$</constant-value>
      </constant>
      <constant>
        <constant-name>zip</constant-name>
        <constant-value>^\d{5}\d*$</constant-value>
      </constant>
      <constant>
        <constant-name>currency</constant-name>
        <constant-value>^\d{1,3}(,?\d{1,3})*\.?(\d{1,2})?$</constant-value>
      </constant> 
   </global>
   
  <formset>
  
  	<form name="loginForm">
        <field property="username" depends="required">
            <arg0 key="loginForm.username"/>
        </field>
        <field property="password" depends="required">
            <arg0 key="loginForm.password"/>
        </field>
    </form>
    
    <form name="modelCharacteristicsForm">
        <field property="modelDescriptor" depends="required">
            <arg0 key="ModelCharacteristicsForm.modelDescriptor"/>
        </field>
        <field property="scientificName" depends="required">
            <arg0 key="ModelCharacteristicsForm.scientificName"/>
        </field>
        <field property="ethinicityStrain" depends="required">
            <arg0 key="ModelCharacteristicsForm.ethinicityStrain"/>
        </field>
        <field property="description" depends="required">
            <arg0 key="ModelCharacteristicsForm.description"/>
        </field>
        <field property="type" depends="required">
            <arg0 key="ModelCharacteristicsForm.type"/>
        </field>
        <field property="releaseDate" depends="required">
            <arg0 key="ModelCharacteristicsForm.releaseDate"/>
        </field>
    </form>

	<form name="geneDeliveryForm">
        <field property="name" depends="required">
            <arg0 key="GeneDeliveryForm.name"/>
    	</field>    
	</form>	

	<form name="inducedMutationForm">
        <field property="name" depends="required">
            <arg0 key="InducedMutationForm.name"/>
    	</field>    
	</form>	
	
	<form name="cellLineForm">
        <field property="cellLineName" depends="required">
            <arg0 key="CellLine.cellLineName"/>
    	</field> 
    	
         <field property="organName" depends="required">
            <arg0 key="CellLine.organName"/>
    	</field>  
	</form>	
	
	<form name="therapyForm">
        <field property="compoundName" depends="required">
            <arg0 key="Therapy.compoundName"/>
    	</field> 
	</form>		

	<form name="engineeredTransgeneForm">
        <field property="locationOfIntegration" depends="required">
            <arg0 key="EngineeredTransgeneForm.locationOfIntegration"/>
    	</field> 
    	<field property="name" depends="required">
            <arg0 key="EngineeredTransgeneForm.name"/>
    	</field> 
    	<field property="transcriptional1_name" depends="required">
            <arg0 key="EngineeredTransgeneForm.transcriptional1_name"/>
    	</field> 
	</form>	

	<form name="genomicSegmentForm">
        <field property="locationOfIntegration" depends="required">
            <arg0 key="GenomicSegmentForm.locationOfIntegration"/>
    	</field> 
	</form>	
	
	<form name="targetedModificationForm">
        <field property="name" depends="required">
            <arg0 key="TargetedModificationForm.name"/>
    	</field> 
	</form>		

	<form name="inducedMutationForm">
        <field property="name" depends="required">
            <arg0 key="InducedMutationForm.name"/>
    	</field> 
	</form>		
	
	<form name="spontaneousMutationForm">
        <field property="name" depends="required">
            <arg0 key="SpontaneousMutationForm.name"/>
    	</field> 
	</form>		
	
	<form name="xenograftForm">
        <field property="name" depends="required">
            <arg0 key="XenograftForm.name"/>
    	</field> 
	</form>		
	
	<form name="publicationForm">
        <field property="authors" depends="required">
            <arg0 key="PublicationForm.authors"/>
    	</field> 
        <field property="firstTimeReported" depends="required">
            <arg0 key="PublicationForm.firstTimeReported"/>
    	</field> 
        <field property="pmid" depends="required">
            <arg0 key="PublicationForm.pmid"/>
    	</field>     	    	
	</form>		
										
  </formset>
</form-validation>
