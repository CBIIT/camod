<CurationStates>

	<State 	symbolicName="INCOMPLETE"
		symbolicNameValue="Incomplete"
		eventName="INCOMPLETE_EVENT"
		eventValue="0"
		nextValidState="CompleteNotScreened"
		default="true"/>		

	<State 	symbolicName="COMPLETE"
		symbolicNameValue="Complete"
		eventName="COMPLETE_EVENT"
		eventValue="1"
		nextValidState="CompleteNotScreened"
		default="false"/>		

	<State 	symbolicName="COMPLETE_NOT_SCREENED"
		symbolicNameValue="CompleteNotScreened"
		eventName="COMPLETE_NOT_SCREENED_EVENT"
		eventValue="2"
		nextValidState="ScreenerAssigned"
		default="false"/>		

	<State 	symbolicName="SCREENER_ASSIGNED"
		symbolicNameValue="ScreenerAssigned"
		eventName="SCREENER_ASSIGNED_EVENT"
		eventValue="3"
		nextValidState="ScreenerApproved, ScreenerRejected"
		default="false"/>		

	<State 	symbolicName="SCREENER_APPROVED"
		symbolicNameValue="ScreenerApproved"
		eventName="SCREENER_APPROVED_EVENT"
		eventValue="4"
		nextValidState="ReviewerAssigned"
		default="false"/>		

	<State 	symbolicName="SCREENER_REJECTED"
		symbolicNameValue="ScreenerRejected"
		eventName="SCREENER_REJECTED_EVENT"
		eventValue="5"
		nextValidState="CompleteNotScreened"
		default="false"/>		

	<State 	symbolicName="REVIEWER_ASSIGNED"
		symbolicNameValue="ReviewerAssigned"
		eventName="REVIEWER_ASSIGNED_EVENT"
		eventValue="6"
		nextValidState="ReviewerApproved, ReviewerRejected"
		default="false"/>		

	<State 	symbolicName="REVIEWER_APPROVED"
		symbolicNameValue="ReviewerApproved"
		eventName="REVIEWER_APPROVED_EVENT"
		eventValue="7"
		nextValidState="Complete"
		default="false"/>		

	<State 	symbolicName="REVIEWER_REJECTED"
		symbolicNameValue="ReviewerRejected"
		eventName="REVIEWER_REJECTED_EVENT"
		eventValue="8"
		nextValidState="CompleteNotScreened"
		default="false"/>		

</CurationStates>