<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.camod.domain">
	<class name="ModificationType" 
    	table="modification_type"
        lazy="true" mutable="false">

	    <!-- id -->
	    <id name="id" 
	    	column="modification_type_id"
	    	type="long"    	
	    	unsaved-value="null">	    	
	        <generator class="native"/>
	    </id>
	    
	    <!-- name -->
        <property name="name" 
        	column="name" 
        	type="string"/>
        	
        <!-- targetedModificationCollection (many-to-many bidirectional) -->
        <!-- Note: this collection will NOT be used to synchronize state with the database. -->
        <!-- Changes made to modificationTypeCollection in TargetedModification will be synchronized with the database. -->
     <!--   <bag name="targetedModificationCollection"
        	table="tar_mod_modification_type"
        	inverse="true" 
        	lazy="true"
        	cascade="all-delete-orphan">
        	
        	<key column="modification_type_id"/>
        	<many-to-many class="TargetedModification" 
        		column="engineered_gene_id"/>
        </bag> -->
	</class>
</hibernate-mapping>