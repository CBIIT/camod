<?xml version="1.0" encoding="UTF-8"?>

<!--
    Document   : AnimalAvailability.hbm.xml
    Created on : July 19, 2005
    Author     : rajputs
    Description: Hibernate mapping file for AnimalAvailability
-->

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gov.nih.nci.camod.domain">
	<class name="AnimalAvailability" 
    	table="animal_availability"
        lazy="true">

	    <!-- id -->
	    <id name="id" 
	    	column="animal_availability_id"
	    	type="long"    	
	    	unsaved-value="null">	    	
	        <generator class="native"/>
	    </id>
	    
	    <!-- name -->
        <property name="name" 
        	column="name" 
        	type="string"/>
        	
        <!-- stockNumber -->
        <property name="stockNumber" 
        	column="stock_number" 
        	type="string"/>
        	
        <!-- animalDistributorCollection (many-to-many bidirectional ) -->
	    <!-- Note: this collection will be used to synchronize state with the database. -->
        <bag name="animalDistributorCollection" 
        	table="ani_ava_ani_distributor"
        	inverse="false"
        	lazy="true" >
        	
        	<key column="animal_availability_id"/>
        	<many-to-many class="AnimalDistributor" 
        		column="animal_distributor_id"/>
        </bag>
	</class>
</hibernate-mapping>